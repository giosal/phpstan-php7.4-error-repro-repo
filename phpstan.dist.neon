parameters:
    editorUrl: 'phpstorm://open?file=%%file%%&line=%%line%%'
    errorFormat: table
    level: 0
    paths:
      - src
      - tests
      - config
      - fixtures
      - migrations
      - templates
      - translations
    doctrine:
      objectManagerLoader: tests/object-manager.php
    symfony:
      containerXmlPath: var/cache/dev/App_KernelDevDebugContainer.xml
